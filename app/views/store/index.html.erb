
<div class="w-full">
  <%= render 'store/notice'%>
  <div class="font-bold">
    Your Pragmatic Catalog
  </div>
  <%= turbo_stream_from 'products' %>

  <ul>
  <% cache @products do %>
   <% @products.each do |product| %>
   <% cache product do %>
      <%= render partial: 'product', object: product %>
   <% end %>
  <% end %>
  <% end %>
</ul>
</div>

